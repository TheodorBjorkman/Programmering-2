<Window x:Class="SP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="5">
        <StackPanel Orientation="Horizontal">
            <Label Margin="5">Ange en text</Label>
            <TextBox Name="testBox" Margin="5" Width="300"></TextBox>
        </StackPanel>
        <Button Click="TestButton" Margin="5" Width="200" Height="30" HorizontalAlignment="Left">
        Test</Button>
        <TextBlock Name="testBlock" Margin="5" Background="#FFF" Height="100" TextWrapping="Wrap">
        </TextBlock>
        <Button Name="btnOpenFile" Click="btnOpenFile_Click">Select file(s)</Button>
        <ListBox Name="txtBox" />
    </StackPanel>
</Window>
